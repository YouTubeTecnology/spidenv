[ req ]
default_bits       = 2048
distinguished_name = req_distinguished_name
req_extensions     = req_ext
x509_extensions    = v3_req
prompt             = no

[ req_distinguished_name ]
C  = IT
ST = Italy
L  = Rome
O  = Protezione Civile Demo
OU = SPID Gateway
CN = spid-gateway.protezionecivile.local

[ req_ext ]
subjectAltName = @alt_names

[ v3_req ]
keyUsage = critical, digitalSignature, keyEncipherment, nonRepudiation
basicConstraints = CA:FALSE
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = localhost
DNS.2 = spid-gateway.protezionecivile.local
URI.1 = https://spid-gateway.protezionecivile.local
